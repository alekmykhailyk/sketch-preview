{
  "name": "Sketch Preview",
  "description": "Preview Artboards in Skala Preview",
  "author": "Marc Schwieterman",
  "homepage": "https://github.com/marcisme/sketch-preview",
  "version": "0.10.0",
  "identifier": "com.marcisme.sketch-preview",
  "compatibleVersion": "3.8.3",
  "bundleVersion": 1,
  "commands": [{
    "name": "Preview",
    "identifier": "preview-command",
    "shortcut": "cmd p",
    "script": "Preview.sketchscript"
  }, {
    "script": "Preview.sketchscript",
    "handlers": {
      "actions": {
        "SelectionChanged.finish": "onSelectionChanged"
      }
    }
  }, {
    "name": "Preview Setup...",
    "identifier": "preview-setup-command",
    "shortcut": "cmd option p",
    "script": "Preview Setup.sketchscript"
  }, {
    "name": "Toggle Auto Preview",
    "identifier": "toggle-auto-preview-command",
    "shortcut": "cmd ctrl p",
    "script": "ToggleAutoPreview.sketchscript"
  }],
  "menu": {
    "items": [
      "preview-command",
      "preview-setup-command",
      "toggle-auto-preview-command"
    ],
    "isRoot": false
  }
}
